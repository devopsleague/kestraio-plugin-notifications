{
    {% if alias is defined %}
        "alias": "{{alias}}",
    {% endif %}
    {% if responders is defined %}
        "responders": [
    {% for responder in responders %}
            ,{
                "id": "{{responder.key | json}}",
                "type": "{{responder.value | json}}"
            }
    {% endfor %}
        ],
    {% endif %}
    {% if visibleTo is defined %}
        "visibleTo": [
    {% for responder in visibleTo %}
        ,{
            "id": "{{responder.key | json}}",
            "type": "{{responder.value | json}}"
        }
    {% endfor %}
        ],
    {% endif %}
    {% if tags is defined %}
        "tags": [
        {% for tag in tags %}
            {{tag | json}}
        {% endfor %}
        ],
    {% endif %}
    {% if priority is defined %}
        "priority": "{{priority}}",
    {% endif %}
    {% if message is defined %}
        "message": "{{message}}",
    {% endif %}
    {% if details is defined %}
        "details": {
            "Namespace": "{{execution.namespace}}",
            "Flow ID": "{{execution.flowId}}",
            "Execution ID": "{{execution.id}}",
            "Execution Status": "{{execution.state.current}}",
            "Final task ID": "{{lastTask.taskId}}"
        },
    {% endif %}
    "description": "*<{{link}}|[{{execution.namespace}}] {{execution.flowId}} âž› {{execution.state.current}} - Final task ID {{lastTask.taskId}}>*\n> {% if firstFailed == false %}Succeeded{% else %}Failed on task `{{firstFailed.taskId}}`{% endif %} after {{duration}}"
}
